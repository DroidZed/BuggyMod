{
  "schemaVersion": 1,
  "id": "buggymod",
  "version": "${version}",
  "name": "BuggyMod",
  "description": "BuggyMod ! Will not make your minecraft buggy (hopefully)! IDK What it does so this will change in time!",
  "authors": [
    "DroidZed"
  ],
  "contact": {
    "homepage": "https://github.com/DroidZed/BuggyMod",
    "sources": "https://github.com/DroidZed/BuggyMod"
  },
  "license": "CC0-1.0",
  "icon": "assets/buggymod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "tn.droidzed.buggymod.BuggyMod",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "tn.droidzed.buggymod.BuggyModClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "tn.droidzed.buggymod.BuggyModDataGenerator",
        "adapter": "kotlin"
      }
    ],
    "modmenu": [
      {
        "value": "tn.droidzed.buggymod.core.config.ModMenuCompat",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "buggymod.mixins.json",
    {
      "config": "buggymod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0",
    "midnightlib": ">=1.4.1-fabric"
  },
  "suggests": {
    "another-mod": "modmenu"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/2UwjH8zvYc",
        "modmenu.twitter": "https://x.com/DroidZed"
      },
      "update_checker": true
    }
  }
}